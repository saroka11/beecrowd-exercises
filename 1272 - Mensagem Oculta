#include <stdio.h>
#include <string.h>

int main(){
    
    int i,j,n,tam;
    char str[52];
    
    scanf("%d",&n);
    getchar();
    
    for(i = 0;i < n;i++){
    gets(str);
    tam =strlen(str);

if(str[0]>96 && str[0]<123)printf("%c",str[0]); // tabela ASCII

   for(j = 1;j < tam;j++){
   
   if(str[j]>96 && str[j]<123){
	  if(str[j-1]<96 || str[j-1]>123) printf("%c",str[j]);
}
}
  printf("\n");
  
    }
    return 0;
}
